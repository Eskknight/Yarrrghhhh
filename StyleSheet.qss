}

/* Customized Spin Box style */

QDoubleSpinBox:focus{
    outline: 0px;
}

QDoubleSpinBox {
    margin-right: 33px;
    margin-left: 0px;
    border-radius: 3px;
    border: 1px solid;
    font-size: 12px;
    color: #2b2d30;
    border-color: #d9dbdf;
    min-height: 30px;
    max-height: 30px;
    min-width: 48px;
    max-width: 48px;
}

QDoubleSpinBox::down-button, QDoubleSpinBox::up-button {
    subcontrol-origin: content;
    width: 20px;
    height: 17px;
    left: 24px;
}

QDoubleSpinBox::down-button {
    bottom: -1px;
    image: url(:/Images/BrushProperty/BrushProperty_indexdown.png);
}

QDoubleSpinBox::up-button {
    top:-1px;
    image: url(:/Images/BrushProperty/BrushProperty_indexup.png);
}

QDoubleSpinBox::up-button:hover{
    image: url(:/Images/BrushProperty/BrushProperty_indexup_hover.png);
}

QDoubleSpinBox::down-button:hover{
    image: url(:/Images/BrushProperty/BrushProperty_indexdown_hover.png);
}

QDoubleSpinBox::up-button:pressed{
    image: url(:/Images/BrushProperty/BrushProperty_indexup_pressed.png);
}

QDoubleSpinBox::down-button:pressed{
    image: url(:/Images/BrushProperty/BrushProperty_indexdown_pressed.png);
}

QDoubleSpinBox:disabled {
    color: #c1c2c3;
}

/* Customized slider style*/

QSlider{
    min-height: 16px;
    max-height: 16px;
    border-radius: 1px;
}

QSlider::groove:horizontal {
height:2px;
border-radius: 1px;
}

QSlider::sub-page:horizontal {
background: #188dfb;
height: 2px;
border-radius: 1px;
}

QSlider::add-page:horizontal {
background: #c7c8ca;

height: 2px;
border-radius: 1px;
}

QSlider::add-page:horizontal:disabled, QSlider::sub-page:horizontal:disabled {
background: #c1c2c3;
}

QSlider::handle:horizontal {
background: white;
border: 1px solid #c7c8ca; 
width: 14px;
background: qradialgradient(spread:pad, cx:0.5, cy:0.5, radius:0.5, fx:0.5, fy:0.5, stop:0.9 white, stop:1.0 #c7c8ca);
margin-top: -7px;
margin-bottom: -7px;
border-radius: 8px;

}
QSlider::handle:pressed{
  background: qradialgradient(spread:pad, cx:0.5, cy:0.5, radius:0.5, fx:0.5, fy:0.5, stop:0.9 #ecedef, stop: 1.0 #e2e5e7);
  }

