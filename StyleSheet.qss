/*--------------------------------------------------------
| Style sheet for vertical resizable window
*---------------------------------------------------------*/
QWidget#verticalResizableWindowFrame {
    background: #efefef;
}

TpQVerticalWindowTitle {
    font-size: 12px;
    color: #2b2d30;
}

TpQVerticalWindowTitle#borderStyled, TpQVerticalWindowTitle#borderStyled[active = "false"] {
    background: #eaebed;
}

TpQVerticalWindowTitle#borderStyled[active = "true"] {
    background: #e2e3e4;
}

TpQVerticalWindowTitle#borderStyled, TpQVerticalWindowTitle#borderStyled[dock="floating"] {
    border: 1px solid #c2c4c6;
}

TpQVerticalWindowTitle#borderStyled[dock="left"] {
    border-top: 1px solid #c2c4c6;
    border-left: none;
    border-right: 1px solid #c2c4c6;
    border-bottom: 1px solid #c2c4c6;
}

TpQVerticalWindowTitle#borderStyled[dock="right"] {
    border-top: 1px solid #c2c4c6;
    border-left: 1px solid #c2c4c6;
    border-right: none;
    border-bottom: 1px solid #c2c4c6;
}

TpQVerticalWindowResizeGrip#borderStyled, TpQVerticalWindowResizeGrip#borderStyled[active = "false"] {
    background: #eaebed;
}

TpQVerticalWindowResizeGrip#borderStyled[active = "true"] {
    background: #e2e3e4;
}

TpQVerticalWindowResizeGrip#borderStyled, TpQVerticalWindowResizeGrip#borderStyled[dock="floating"] {
    border: 1px solid #c2c4c6;
}

TpQVerticalWindowResizeGrip#borderStyled[dock="left"] {
    border-bottom: 1px solid #c2c4c6;
    border-left: none;
    border-right: 1px solid #c2c4c6;
    border-bottom: 1px solid #c2c4c6;
}

TpQVerticalWindowResizeGrip#borderStyled[dock="right"] {
    border-bottom: 1px solid #c2c4c6;
    border-left: 1px solid #c2c4c6;
    border-right: none;
    border-bottom: 1px solid #c2c4c6;
}

QFrame#verticalResizableWindowBodyContainer, QFrame#verticalResizableWindowBodyContainer[dock="floating"] {
    background: #efefef;
    border-left: 1px solid #c2c4c6;
    border-right: 1px solid #c2c4c6;
}

QFrame#verticalResizableWindowBodyContainer[dock="left"] {
    border-left: none;
    border-right: 1px solid #c2c4c6;
}

QFrame#verticalResizableWindowBodyContainer[dock="right"] {
    border-left: 1px solid #c2c4c6;
    border-right: none;
}

QWidget#verticalResizableWindowContent{
    background-color: #efefef;
}


